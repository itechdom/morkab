{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/**/!(*.@(js|ts|html|css|json|svg|png|jpg|jpeg))",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ],
    "ignore": [
      "firebase.json"
    ]
  }
}
